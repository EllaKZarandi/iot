{
  "name": "sensors-backend",
  "version": "1.0.0",
  "description": "Backend services for storing sensors data and providing the corresonding APIs for them",
  "main": "dist/index.js",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint -c .eslintrc.js --fix --ext .ts ./",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "speed-and-cadence-data-processor": "ts-node src/services/sensor-data-processors/speedAndCadence.ts",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\""
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongodb": "^4.9.0",
    "mongoose": "^6.5.3",
    "mqtt": "^4.3.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.184",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
